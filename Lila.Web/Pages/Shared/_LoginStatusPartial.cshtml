@if (User.Identity.IsAuthenticated)
{
    <form method="post" class="form-inline" asp-page="/Account/Logout">
        <button type="submit" class="btn btn-outline-primary pe-4 ps-4">Log Out</button>
    </form>    
}
else
{
    <a class="btn btn-outline-primary pe-4 ps-4" asp-page="/Account/Login">Log In</a>
}
